# ╔═╡ d6cc3a7d-6cad-4ff5-b601-4209a494abbe
struct Review
    datetime::DateTime
    grade::String
end

# ╔═╡ 7b014fda-366f-11ee-2ab1-c95a638d2b04
struct Card
    spelling::String
    reading::String
